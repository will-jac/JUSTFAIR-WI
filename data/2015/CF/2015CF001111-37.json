lication,\n    <cite><a href=\"https://dwd.wisconsin.gov/er/civilrights/discrimination/arrest.htm\" rel=\"noopener noreferrer\" target=\"_blank\">Arrest and Conviction Records under the Law</a></cite>.\n  </p>","warrants":[],"civilJdgmts":[{"dateDkted":"2021-07-22","satisfaction":"No","parties":[{"name":"Patrick T Forqurean","bankrupt":false,"released":false,"descr":"Debtor","centralAtty":null,"proHacVice":null,"key":"58ac4ab3680238cb98cc9814d139cdf9025aef7e-1","status":"Active","address":"301 Troy Drive, Mendota Mental Health Institute, Madison, WI 53704"},{"name":"Marathon County Clerk of Circuit Court","bankrupt":null,"released":false,"descr":"Creditor","centralAtty":null,"proHacVice":null,"key":"58ac4ab3680238cb98cc9814d139cdf9025aef7e-2","status":"Active","address":null}],"jdgmtDate":"2021-07-22","propDescr":null,"descr":"Judgment for money","typeOfTax":null,"amount":679.90,"key":"58ac4ab3680238cb98cc9814d139cdf9025aef7e","svcDateTo":null,"cvJdgmtEvTypeDescr":null,"svcDateFrom":null,"multiple":false,"fullSatisDate":null,"dktedTime":"02:02 pm","dbtrName":"Forqurean, Patrick T","warrantNo":null,"events":[],"amounts":[{"key":"58ac4ab3680238cb98cc9814d139cdf9025aef7e-1","descr":"Judgment amount","amount":650.00},{"key":"58ac4ab3680238cb98cc9814d139cdf9025aef7e-2","descr":"Post-judgment interest","amount":2.30},{"key":"58ac4ab3680238cb98cc9814d139cdf9025aef7e-3","descr":"Post-judgment interest","amount":2.30},{"key":"58ac4ab3680238cb98cc9814d139cdf9025aef7e-4","descr":"Post-judgment interest","amount":2.30},{"key":"58ac4ab3680238cb98cc9814d139cdf9025aef7e-5","descr":"Post-judgment interest","amount":2.30},{"key":"58ac4ab3680238cb98cc9814d139cdf9025aef7e-6","descr":"Post-judgment interest","amount":2.30},{"key":"58ac4ab3680238cb98cc9814d139cdf9025aef7e-7","descr":"Post-judgment interest","amount":2.30},{"key":"58ac4ab3680238cb98cc9814d139cdf9025aef7e-8","descr":"Post-judgment interest","amount":2.30},{"key":"58ac4ab3680238cb98cc9814d139cdf9025aef7e-9","descr":"Post-j