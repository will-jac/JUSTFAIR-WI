oday.","parties":[],"descr":"Hearing","amount":null,"key":"14-2014CF000401-46","ctofc":"Sciascia, Joseph G.","date":"2016-01-08","courtRptr":"Keel, Kimberly"},{"addlTxt":"Inmate Request Slip","parties":[],"descr":"Other papers","amount":null,"key":"14-2014CF000401-49","ctofc":null,"date":"2016-01-05","courtRptr":null},{"addlTxt":"Hearing at 01-08-2016 09:30 am","parties":[],"descr":"Notice of hearing","amount":null,"key":"14-2014CF000401-44","ctofc":null,"date":"2015-12-28","courtRptr":null},{"addlTxt":"from Atty Fischer","parties":[],"descr":"Letters/correspondence","amount":null,"key":"14-2014CF000401-43","ctofc":null,"date":"2015-12-23","courtRptr":null},{"addlTxt":"character letters","parties":[],"descr":"Letters/correspondence","amount":null,"key":"14-2014CF000401-45","ctofc":null,"date":"2015-12-16","courtRptr":null},{"addlTxt":"Order Revoking Huber Law Work Release Privileges","parties":[],"descr":"Order","amount":null,"key":"14-2014CF000401-42","ctofc":"Sciascia, Joseph G.","date":"2015-12-16","courtRptr":null},{"addlTxt":"for revocation of huber","parties":[],"descr":"Request","amount":null,"key":"14-2014CF000401-41","ctofc":null,"date":"2015-12-16","courtRptr":null},{"addlTxt":"for finding of contempt of court for failure to pay attorney services fees, for imposition of remedial sanctions, for costs on motion and for judgment; affidavit in support of motions; and order for hearing","parties":[],"descr":"Notice of motion, motion","amount":null,"key":"14-2014CF000401-40","ctofc":null,"date":"2015-12-10","courtRptr":null},{"addlTxt":"Per post office card\nADDRESS INFO for Chad E Douglas\nCurrent: 1506 E Main St, Watertown, WI 53094 United States  (Effective: 12-08-2015)\nPrior: 1221 Oak St, Watertown, WI 53098 \n","parties":[{"sealed":false,"name":"Douglas, Chad E"}],"descr":"Change of address notification","amount":null,"key":"14-2014CF000401-39","ctofc":null,"date":"2015-12-08","courtRptr":null},{"addlTxt":"modifying JOC","parties":[],"descr":"Order","amoun